<launch>
  <!-- Run your RRT* planner -->
  <node pkg="hector_rrt_star" type="rrt_star_bidirectional" name="rrt_star_planner" output="screen" />

  <!-- Run DecompROS Node -->
  <node pkg="decomp_test_node"
    type="test_path_decomp_3d"
    name="test_decomp"
    output="screen">
    
    <!-- Subscribe to your RRT* path -->
    <remap from="/path" to="/rrt_bid_path"/>

    <!-- Subscribe to your PCL map -->
    <remap from="/cloud" to="/safe_flight_corridors/pointcloud"/>

    <!-- Optionally specify a voxel resolution for downsampling -->
    <!-- <param name="map_resolution" value="0.1"/> -->

  </node>

</launch>
